<div id="<%= dom_id coin %>">
  <p>
    <strong>Descrição:</strong>
    <%= coin.description %>
  </p>

  <p>
    <strong>Sigla:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Imagem:</strong>
    <%= image_tag coin.url_image, width: 25, heigth: 25 %>
    <%= coin.url_image %>
  </p>

   <p>
    <strong>Tipo de mineração:</strong>
    <%= coin.mining_type.description %>
  </p>

</div>
